<%
//========================================================================
//Page d'accueil
%>
<% let data = obj %>

<% if (data.kind === "package") { %>
    <h3><%= data.name %> <%= data.version %></h3>
<% } %>

<% if (data.readme) { %>
    <section>
        <div class="readme">
            <%= data.readme %>
        </div>
    </section>
<% } %>
